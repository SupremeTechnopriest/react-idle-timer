{
  "name": "react-idle-timer",
  "version": "2.0.0",
  "description": "Activity detection for React.js",
  "main": "build/index.js",
  "files": [
    "build",
    "examples"
  ],
  "scripts": {
    "clean": "run-s rimraf-build rimraf-examples-app rimraf-examples-init rimraf-examples-server",
    "rimraf-build": "rimraf build",
    "rimraf-examples-app": "rimraf examples/App.js",
    "rimraf-examples-init": "rimraf examples/init.js",
    "rimraf-examples-server": "rimraf examples/server.js",
    "build": "cross-env NODE_ENV=production run-s babel examples",
    "babel": "babel src -d build",
    "examples": "babel src_examples -d examples",
    "rebuild": "run-s clean build",
    "start": "cross-env NODE_ENV=development node examples/server",
    "test": "npm test",
    "release": "standard-version",
    "package": "run-s rebuild version-patch publish",
    "version-patch": "npm version patch",
    "publish": "npm publish --tag latest"
  },
  "keywords": [
    "react",
    "idle",
    "idletimer",
    "timer",
    "activity",
    "active"
  ],
  "author": "Randy Lebeau",
  "contributors": [
    "Cole Chamberlain <cole.chamberlain@gmail.com> (https://github.com/cchamberlain)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/supremetechnopriest/react-idle-timer.git"
  },
  "dependencies": {
    "date-fns": "^1.28.5",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "peerDependencies": {
    "react": "^16.x.x",
    "react-dom": "^16.x.x",
    "prop-types": "^15.x.x"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^5.1.3",
    "del": "^1.2.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-eslint": "^0.14.0",
    "gulp-jest": "^0.4.0",
    "gulp-shell": "^0.4.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "jest-cli": "^0.4.13",
    "npm-run-all": "^4.1.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.0",
    "rimraf": "^2.4.3",
    "standard-version": "^2.4.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
